{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node", "@types/chai"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true
  },
  "include": ["**/*.ts", "support/*.ts"]
} 